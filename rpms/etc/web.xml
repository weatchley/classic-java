    </servlet-mapping>

    <servlet>
        <servlet-name>doHomePage</servlet-name>
        <servlet-class>gov.ymp.rpms.doHomePage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>doHomePage</servlet-name>
        <url-pattern>/doHomePage</url-pattern>
    </servlet-mapping>


    <!-- Environment Entries ******************************************** -->
    <env-entry>
      <env-entry-name>SystemName</env-entry-name>
      <env-entry-value>Records Plan Management System</env-entry-value>
      <env-entry-type>java.lang.String</env-entry-type>
    </env-entry>
    
    <env-entry>
      <env-entry-name>SystemAcronym</env-entry-name>
      <env-entry-value>rpms</env-entry-value>
      <env-entry-type>java.lang.String</env-entry-type>
    </env-entry>
    
    <env-entry>
      <env-entry-name>AppSchema</env-entry-name>
      <env-entry-value>rpms</env-entry-value>
      <env-entry-type>java.lang.String</env-entry-type>
    </env-entry>
    
</web-app>
