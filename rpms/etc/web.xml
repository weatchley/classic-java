<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- Development version of web.xml file -->

<web-app>

    <!-- Set up ******************************************** -->
    <display-name>rpms</display-name>
    <description>
	Record Plan Management System
    </description>

    <session-config>
      <session-timeout>240</session-timeout>
    </session-config>

    <welcome-file-list>
      <welcome-file>/index.jsp</welcome-file>
    </welcome-file-list>

    <!-- Servlet config/mapping ******************************************** -->
    <servlet>
        <servlet-name>rpms</servlet-name>
        <servlet-class>gov.ymp.rpms</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rpms</servlet-name>
        <url-pattern>/rpms</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>doLogin</servlet-name>
        <servlet-class>gov.ymp.rpms.doLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>doLogin</servlet-name>
        <url-pattern>/doLogin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>doNewsletter</servlet-name>
        <servlet-class>gov.ymp.rpms.doNewsletter</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>doNewsletter</servlet-name>
        <url-pattern>/doNewsletter</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>doFAQ</servlet-name>
        <servlet-class>gov.ymp.rpms.doFAQ</servlet-class>
    </servlet>
    <servlet-mapping>
