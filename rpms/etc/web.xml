    </servlet>
    <servlet-mapping>
        <servlet-name>doRetentionPeriod</servlet-name>
        <url-pattern>/doRetentionPeriod</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>doARestriction</servlet-name>
        <servlet-class>gov.ymp.rpms.doARestriction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>doARestriction</servlet-name>
        <url-pattern>/doARestriction</url-pattern>
    </servlet-mapping>

    <servlet>
